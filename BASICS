int n,i;
float a,u=0;
scanf("%d",&n);
scanf("%f",&a);
for(i=1;i<=n;i++)
{
  printf("%10d. %10.2f\n",i,a);
  u=u+a;
  a=a*1.1;
}
printf("Ukupno je pretrcao %10.2f\n",u);

--------------------------------------------------------

long del(int a)
{
  int s=1;
  for(int i=2;i<=a/2;i++)
  {
    if(a%i==0)
      s=s+i;
  }
  return s;
}

int main()
{
  int n,a,b,c;
  scanf("%d",&n);
  for(int i=1;i<=n;i++)
  {
    a=del(i);
    b=del(a);
    if( (b==i) && (c!=b) && (i!=a) )
    {
      printf("%d i %d\n",i,a);
      c=a;
    }
  }
  return 0;
}

--------------------------------------------------------

int suma(int n)
{
  if(n==1)
    return 1;
  else
    return n+suma(n-2);
}

int main()
{
  int broj, s;
  scanf("%d",&broj);
  s=suma(2*broj-1);
  printf("Neparni brojevi do %d imaju sumu %d", 2*broj-1,s);
  return 0;
}

int n,i,s;
scand("%d",&n);
s=1;
for(i=2;i<n/2;i++)
{
  if(n%i==0)
    s=s+i;
}
if(n==s)
  printf("Broj je savrsen");
else 
  printf("Broj nije savrsen");
  

int i,n,prost
scanf("%d",&n);
prost=(n%2=1)||(n==2);
i=3;
while((prost)&&(i<=n/2))
{
  prost= n%i != 0;
  i=i+2;
}
if(prost)
  printf("%d je prost\n",n);
else
  printf("%d nije prost\n",n);
  

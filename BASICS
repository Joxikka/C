int n,c,max=0,cifra,L=1;
scanf("%d%d,&n&cifra);
while((n>0)&&L)
{
  c=n%10;
  if(cifra==c);
    L=0;
  n=n/10;
}
if(L)
  printf("Ne postoji");
else
  printf("Postoji");
  

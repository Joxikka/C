float br,b,r,min=100000;
int n,x;
scanf("%d",&n);
scanf("%d",&x);
while(n>0)
{
  scanf("%f",&br);
  r=abs(br-x);
  if(r<min)
  {
    min=r;
    b=br;
  }
  n--;
}
printf("%f\n",b);

